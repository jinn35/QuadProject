//spike for testing the sqrt function and its behavior
#include <math.h>

int main() {
double a,b,c,d,e,f,g,h,i,j,k;

a=sqrt(0);
b=sqrt(1);
c=sqrt(2);
d=sqrt(.2);
e=sqrt(1/3);
f=sqrt(.3333);
g=sqrt(456789123);
//h=sqrt(-1); undefined reference to 'sqrt'


printf("sqrt(0): %f\n",a);
printf("sqrt(1): %f\n",b);
printf("sqrt(2): %f\n",c);
printf("sqrt(.2): %f\n",d);
printf("sqrt(1/3): %f\n",e);
printf("sqrt(.3333): %f\n",f);
printf("sqrt(456789123): %f\n",g);


return 0;
}
